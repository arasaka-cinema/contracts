{
  "name": "@arasaka-cinema/contracts",
  "version": "1.0.0",
  "description": "Protobuf definitions and generated TypeScript types for Arasaka Cinema",
  "scripts": {
    "generate": "protoc -I ./proto ./proto/*.proto --ts_proto_out=./gen --ts_proto_opt=nestJs=true,package=omit"
  },
  "files": [
    "gen",
    "proto"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/microservices": "^11.1.12",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.11.1"
  }
}
